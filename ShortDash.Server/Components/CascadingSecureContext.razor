<CascadingValue Value="@ServerPublicKey" Name="ServerPublicKey">
    <CascadingValue Value="@ClientPublicKey" Name="ClientPublicKey">
        @if (!IsInitialized)
        {
            <PageLoadingComponent />
        }
        else
        {
            @ChildContent
        }
    </CascadingValue>
</CascadingValue>