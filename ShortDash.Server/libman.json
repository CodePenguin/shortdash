{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@5.14.0",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css",
        "webfonts/*"
      ]
    },
    {
      "library": "twitter-bootstrap@4.5.2",
      "destination": "wwwroot/lib/twitter-bootstrap/",
      "files": [
        "css/bootstrap.min.css",
        "js/bootstrap.min.js"
      ]
    },
    {
      "library": "qrcodejs@1.0.0",
      "destination": "wwwroot/lib/qrcodejs/",
      "files": [
        "qrcode.min.js"
      ]
    },
    {
      "library": "forge@0.10.0",
      "destination": "wwwroot/lib/forge/",
      "files": [
        "forge.min.js"
      ]
    }
  ]
}