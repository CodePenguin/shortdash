{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@5.14.0",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css",
        "webfonts/*"
      ]
    },
    {
      "library": "twitter-bootstrap@4.5.2",
      "destination": "wwwroot/lib/twitter-bootstrap/",
      "files": [
        "css/bootstrap.min.css",
        "js/bootstrap.min.js"
      ]
    },
    {
      "library": "jsencrypt@2.3.1",
      "destination": "wwwroot/lib/jsencrypt/",
      "files": [
        "jsencrypt.min.js"
      ]
    },
    {
      "library": "crypto-js@4.0.0",
      "destination": "wwwroot/lib/crypto-js/",
      "files": [
        "crypto-js.min.js"
      ]
    }
  ]
}